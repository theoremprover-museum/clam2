problem([]==>a:int=>count(a,nil)=0 in pnat,
repeat dequantify_once,lambda(a,_7650),
[problem([a:int]==>count(a,nil)=0 in pnat,
 simplify then repeat intro,axiom,
 [
 ]) ext _7650
]).
