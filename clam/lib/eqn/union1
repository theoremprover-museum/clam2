problem([]==>l2:int list=>union(nil,l2)=l2 in int list,
repeat intro,lambda(l2,_7807),
[problem([l2:int list]==>union(nil,l2)=l2 in int list,
 simplify then repeat intro,axiom,
 [
 ]) ext _7807
]).
