/* This is the pretty-printed form
ind_strat([(x:int list)-v1::v0]) then 
  base_case(...)

*/

proof_plan([]==>x:int list=>y:int list=>length(app(x,y))=plus(length(x),length(y))in pnat,lensum,1090,ind_strat(induction(lemma(list_primitive)-[(x:int list)-v1::v0])then[base_case(normalize_term([reduction([1,1,1],[app1,equ(int list,left)]),reduction([1,2,1],[length1,equ(pnat,left)]),reduction([2,1],[plus1,equ(pnat,left)])])then[elementary((intro(new[y])then wfftacs)then unfold_iff then identity)]),step_case(ripple(direction_out,wave(direction_out,[1,2,1],[length2,equ(pnat,left)],[])then[wave(direction_out,[2,1],[plus2,equ(pnat,left)],[])then[wave(direction_out,[1,1,1],[app2,equ(int list,left)],[])then[wave(direction_out,[1,1],[length2,equ(pnat,left)],[])]]])then[unblock_then_fertilize(weak,unblock_fertilize_lazy([idtac])then fertilize(weak,pwf_then_fertilize(weak,fertilize_left_or_right([weak_fertilize(right,in,[1],v2)]))then[idtac]))])])then[base_case(elementary((intro(new[y])then wfftacs)then unfold_iff then identity))],dplan).
