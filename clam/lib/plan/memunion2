proof_plan([]==>x:int=>a:int list=>b:int list=>member(x,b)=>member(x,union(a,b)),memunion2,1180,ind_strat(induction(lemma(list_primitive)-[(a:int list)-v1::v0])then[base_case(normalize_term([reduction([2,2],[union1,equ(int list,left)])])then[elementary((intro(new[x])then wfftacs)then(intro(new[b])then wfftacs)then unfold_iff then(intro(new[_132382])then wfftacs)then hyp(_132382))]),step_case(ripple(direction_out,casesplit(disjunction([member(v1,b)=>void,member(v1,b)]))then[wave(direction_out,[2,2],[union4,equ(int list,left)],[])then[casesplit(disjunction([x=v1 in int=>void,x=v1 in int]))then[wave(direction_out,[2],[member3,equ(u(1),left)],[]),wave(direction_out,[2],[member2,complementary,equ(u(1),left)],[])]],wave(direction_out,[2,2],[union3,equ(int list,left)],[])])then[unblock_then_fertilize(strong,unblock_fertilize_lazy([idtac])then fertilize(strong,pwf_then_fertilize(strong,fertilization_strong(v2)))),idtac,unblock_then_fertilize(strong,unblock_fertilize_lazy([idtac])then fertilize(strong,pwf_then_fertilize(strong,fertilization_strong(v2))))])])then[base_case(elementary(unfold_iff then(intro(new[_132178])then wfftacs)then intro(explicit(0))then simplify then wfftacs))],dplan).
