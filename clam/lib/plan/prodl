proof_plan([]==>l:pnat list=>m:pnat list=>prodl(pnatapp(l,m))=times(prodl(l),prodl(m))in pnat,prodl,4750,ind_strat(induction(lemma(list_primitive)-[(l:pnat list)-v1::v0])then[base_case(normalize_term([reduction([1,1,1],[pnatapp1,equ(pnat list,left)]),reduction([1,2,1],[prodl1,equ(pnat,left)]),reduction([2,1],[times2,equ(pnat,left)]),reduction([1,2,1],[times1,equ(pnat,left)]),reduction([2,1],[plus1,equ(pnat,left)])])then[elementary((intro(new[m])then wfftacs)then unfold_iff then identity)]),step_case(ripple(direction_out,wave(direction_out,[1,2,1],[prodl2,equ(pnat,left)],[])then[wave(direction_out,[1,1,1],[pnatapp2,equ(pnat list,left)],[])then[wave(direction_out,[1,1],[prodl2,equ(pnat,left)],[])]])then[unblock_then_fertilize(weak,unblock_fertilize_lazy([idtac])then fertilize(weak,pwf_then_fertilize(weak,fertilize_left_or_right([weak_fertilize(left,in,[2],v2)]))then[idtac]))])])then[generalise(prodl(m),v3:pnat)then[generalise(prodl(v0),v4:pnat)then[ind_strat(induction(lemma(pnat_primitive)-[(v1:pnat)-s(v5)])then[base_case(normalize_term([reduction([1,1],[times1,equ(pnat,left)]),reduction([1,2,1],[times1,equ(pnat,left)]),reduction([2,1],[times1,equ(pnat,left)])])then[elementary((intro(new[v4])then wfftacs)then(intro(new[v3])then wfftacs)then(intro(new[m])then wfftacs)then unfold_iff then identity)]),step_case(ripple(direction_out,wave(direction_out,[1,1],[times2,equ(pnat,left)],[])then[wave(direction_out,[1,2,1],[times2,equ(pnat,left)],[])then[wave(direction_out,[2,1],[disttwo,equ(pnat,left)],[])]])then[unblock_then_fertilize(weak,unblock_fertilize_lazy([idtac])then fertilize(weak,pwf_then_fertilize(weak,fertilize_left_or_right([weak_fertilize(right,in,[1],v6)]))then[idtac]))])])then[base_case(elementary((intro(new[v4])then wfftacs)then(intro(new[v3])then wfftacs)then(intro(new[m])then wfftacs)then unfold_iff then identity))]]]],dplan).
